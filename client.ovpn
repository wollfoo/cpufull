client
dev tun
proto udp
remote 47.238.77.233 1194   # Địa chỉ IP của máy chủ VPN
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
cipher AES-256-CBC
data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305
auth SHA256
key-direction 1
verb 3

# File chứng chỉ và khóa
ca ca.crt
cert client.crt
key client.key
tls-auth ta.key 1
